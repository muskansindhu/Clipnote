{
  "name": "clipnote",
  "version": "2.0.0",
  "description": "saves timestamp notes for youtube videos and provide summaries",
  "manifest_version": 3,
  "author": "Muskan Sindhu",
  "action": {
    "default_popup": "login.html"
  },
  "permissions": ["activeTab", "tabs", "scripting", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "externally_connectable": {
    "matches": ["http://127.0.0.1:5001/*", "http://localhost:5001/*"]
  }
}
